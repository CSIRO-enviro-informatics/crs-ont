#################################################################
#    Object Properties
#################################################################

###  http://linked.data.gov.au/def/crs#agency
:agency rdf:type owl:ObjectProperty ;
        rdfs:range org:Organization .


###  http://linked.data.gov.au/def/crs#associatedWith
:associatedWith rdf:type owl:ObjectProperty ;
                rdfs:domain :Agent ;
                rdfs:range :Agent .


###  http://linked.data.gov.au/def/crs#mandatedBy
:mandatedBy rdf:type owl:ObjectProperty ;
            rdfs:range :Authority .


###  http://linked.data.gov.au/def/crs#performs
:performs rdf:type owl:ObjectProperty ;
          rdfs:range <http://reference.data.gov.au/def/ont/agrif#Function> .


###  http://linked.data.gov.au/def/crs#previousAgency
:previousAgency rdf:type owl:ObjectProperty ;
                owl:inverseOf :subsequentAgency .


###  http://linked.data.gov.au/def/crs#subordinateAgency
:subordinateAgency rdf:type owl:ObjectProperty ;
                   owl:inverseOf :superiorAgency .


###  http://linked.data.gov.au/def/crs#subsequentAgency
:subsequentAgency rdf:type owl:ObjectProperty .


###  http://linked.data.gov.au/def/crs#superiorAgency
:superiorAgency rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://linked.data.gov.au/def/crs#controlNumber
:controlNumber rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf org:identifier ;
               rdfs:range xsd:string .


###  http://www.w3.org/ns/org#identifier
org:identifier rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://linked.data.gov.au/def/crs#AdministrativeArrangementOrder
:AdministrativeArrangementOrder rdf:type owl:Class ;
                                rdfs:isDefinedBy <http://test.linked.data.gov.au/def/crs> ;
                                rdfs:label "Administrative Arrangement Order"@en .


###  http://linked.data.gov.au/def/crs#Agency
:Agency rdf:type owl:Class ;
        rdfs:subClassOf :Agent ,
                        org:Organization .


###  http://linked.data.gov.au/def/crs#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf <http://xmlns.com/foaf/0.1/Agent> .


###  http://linked.data.gov.au/def/crs#Association
:Association rdf:type owl:Class ;
             rdfs:comment "Associations are temporal assocations between CRS Agents, for instance a Commonwealth Person may be assocaited with an Agency for 3 particular years."@en ;
             rdfs:label "Association" .


###  http://linked.data.gov.au/def/crs#Authority
:Authority rdf:type owl:Class ;
           rdfs:comment "An authority or rule that define the functions and activities of a Government Entity"@en ;
           rdfs:isDefinedBy <http://test.linked.data.gov.au/def/crs> ;
           rdfs:label <http://linked.data.gov.au/def/Authority> .


###  http://linked.data.gov.au/def/crs#CommonwealthPerson
:CommonwealthPerson rdf:type owl:Class ;
                    rdfs:subClassOf :Agent ,
                                    <http://xmlns.com/foaf/0.1/Person> ;
                    rdfs:isDefinedBy <http://test.linked.data.gov.au/def/crs> .


###  http://linked.data.gov.au/def/crs#Function
:Function rdf:type owl:Class ;
          owl:equivalentClass <http://reference.data.gov.au/def/ont/agrif#Function> ;
          rdfs:comment "A major function normally undertaken by an agency in its operations"@en ;
          rdfs:label "Government Function"@en .


###  http://linked.data.gov.au/def/crs#Item
:Item rdf:type owl:Class .


###  http://linked.data.gov.au/def/crs#Organisation
:Organisation rdf:type owl:Class ;
              rdfs:subClassOf :Agent ,
                              org:Organization .


###  http://linked.data.gov.au/def/crs#Series
:Series rdf:type owl:Class ;
        rdfs:isDefinedBy <http://test.linked.data.gov.au/def/crs> .


###  http://reference.data.gov.au/def/ont/agrif#AGIFTFunction
<http://reference.data.gov.au/def/ont/agrif#AGIFTFunction> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://reference.data.gov.au/def/ont/agrif#Function> ;
                                                           rdfs:isDefinedBy <http://reference.data.gov.au/def/ont/agrif> ;
                                                           rdfs:label "AGIFT Function"^^xsd:string .


###  http://reference.data.gov.au/def/ont/agrif#Function
<http://reference.data.gov.au/def/ont/agrif#Function> rdf:type owl:Class ;
                                                      rdfs:comment "A Function is a process that is performed routinely to carry out a part of the mandate of an Australian Government Agency."^^xsd:string ;
                                                      rdfs:isDefinedBy <http://reference.data.gov.au/def/ont/agrif> ;
                                                      rdfs:label "Function"^^xsd:string .


###  http://reference.data.gov.au/def/ont/agrif#RecordsAuthorityFunction
<http://reference.data.gov.au/def/ont/agrif#RecordsAuthorityFunction> rdf:type owl:Class ;
                                                                      rdfs:subClassOf <http://reference.data.gov.au/def/ont/agrif#Function> ;
                                                                      rdfs:isDefinedBy <http://reference.data.gov.au/def/ont/agrif> ;
                                                                      rdfs:label "Records Authority Function"^^xsd:string .


###  http://www.w3.org/ns/org#Organization
org:Organization rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Agent
<http://xmlns.com/foaf/0.1/Agent> rdf:type owl:Class .


