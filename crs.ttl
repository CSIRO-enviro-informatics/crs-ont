# baseURI: http://linked.data.gov.au/def/crs
# imports: http://linked.data.gov.au/def/auorg
# prefix: crs

@prefix auorg: <http://linked.data.gov.au/def/auorg/> .
@prefix crs: <http://linked.data.gov.au/def/crs#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix organization: <http://ontology.eil.utoronto.ca/organization.owl> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://linked.data.gov.au/def/crs>
  rdf:type owl:NamedIndividual ;
  rdf:type owl:Ontology ;
  <http://purl.org/dc/elements/1.1/contributor> "David Hearder" ;
  <http://purl.org/dc/elements/1.1/contributor> "Marco Wallenius" ;
  <http://purl.org/dc/elements/1.1/contributor> "Simon J.D. Cox" ;
  <http://purl.org/dc/elements/1.1/creator> "Nicholas J. Car" ;
  <http://purl.org/dc/elements/1.1/publisher> "National Archives of Australia" ;
  terms:contributor <https://orcid.org/0000-0002-3884-3420> ;
  terms:created "2018-09-10"^^xsd:date ;
  terms:creator <http://orcid.org/0000-0002-8742-7730> ;
  terms:modified "2018-11-14"^^xsd:date ;
  terms:publisher <http://catalogue.linked.data.gov.au/org/O-000790> ;
  rdfs:comment """This ontology is an [OWL](https://www.w3.org/OWL/) interpretation of the Commonwealth Record Series (CRS) that is maintained by the [National Archives of Australia](http://naa.gov.au).

This ontology specialises classes and properties from the [Organization Ontology](https://www.w3.org/TR/vocab-org/) and other well-known ontologies such as [Dublin Core](http://dublincore.org/schemas/rdfs/) & [PROV](https://www.w3.org/TR/prov-o/).

It also reuses many components from the following Australian government ontologies:

* [Au Org Ontology](https://github.com/CSIRO-enviro-informatics/auorg-ont) - Australian government structures
* [AGRIF](http://reference.data.gov.au/def/ont/agrif) - Australian government Functions & Records""" ;
  rdfs:label "AU Org Ontology"@en ;
  rdfs:seeAlso "The GitHub repository containing addition documentation: http://github.com/CSIRO-enviro-informatics/auorg-ont/"@en ;
  owl:imports <http://linked.data.gov.au/def/auorg> ;
  owl:versionIRI <http://linked.data.gov.au/def/crs#1.0> ;
  owl:versionInfo "Beta version"@en ;
.
crs:AdministrativeArrangementOrder
  rdf:type owl:Class ;
  rdfs:isDefinedBy <http://linked.data.gov.au/def/crs> ;
  rdfs:label "Administrative Arrangement Order"@en ;
  owl:equivalentClass <https://linked.data.gov.au/ont/aao/AAO> ;
.
crs:Agency
  rdf:type owl:Class ;
  rdfs:subClassOf crs:Agent ;
  rdfs:subClassOf org:Organization ;
  owl:equivalentClass auorg:Agency ;
.
crs:Agent
  rdf:type owl:Class ;
  rdfs:subClassOf <http://xmlns.com/foaf/0.1/Agent> ;
  owl:equivalentClass <http://www.w3.org/ns/prov#Agent> ;
.
crs:AssistantMinister
  rdf:type owl:NamedIndividual ;
  owl:sameAs crs:ParliamentarySecretary ;
.
crs:Association
  rdf:type owl:Class ;
  rdfs:comment "Associations are temporal assocations between CRS Agents, for instance a Commonwealth Person may be assocaited with an Agency for 3 particular years."@en ;
  rdfs:label "Association" ;
  rdfs:subClassOf org:Membership ;
.
crs:Authority
  rdf:type owl:Class ;
  rdfs:comment "An authority or rule that define the functions and activities of a Government Entity"@en ;
  rdfs:isDefinedBy <http://linked.data.gov.au/def/crs> ;
  rdfs:label <http://linked.data.gov.au/def/Authority> ;
.
crs:CommonwealthPerson
  rdf:type owl:Class ;
  rdfs:isDefinedBy <http://linked.data.gov.au/def/crs> ;
  rdfs:subClassOf crs:Agent ;
  rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
.
crs:DeputyPrimeMinister
  rdf:type owl:NamedIndividual ;
.
crs:Function
  rdf:type owl:Class ;
  rdfs:comment "A major function normally undertaken by an agency in its operations"@en ;
  rdfs:label "Government Function"@en ;
  owl:equivalentClass <http://reference.data.gov.au/def/ont/agrif#Function> ;
.
crs:Item
  rdf:type owl:Class ;
.
crs:Member
  rdf:type owl:NamedIndividual ;
.
crs:Minister
  rdf:type owl:NamedIndividual ;
.
crs:Organisation
  rdf:type owl:Class ;
  rdfs:subClassOf crs:Agent ;
  rdfs:subClassOf org:Organization ;
.
crs:ParliamentarySecretary
  rdf:type owl:NamedIndividual ;
.
crs:PrimeMinister
  rdf:type owl:NamedIndividual ;
.
crs:Role
  rdf:type owl:Class ;
  rdfs:comment "The role played by one CRS Agent when associated with another via an Assotiation"@en ;
  rdfs:label "Role"@en ;
.
crs:Series
  rdf:type owl:Class ;
  rdfs:isDefinedBy <http://linked.data.gov.au/def/crs> ;
.
crs:associatedWith
  rdf:type owl:ObjectProperty ;
  rdfs:comment "An Agent (Commonwealth Person, Agency, Organisation) is associated with another Agent." ;
  rdfs:domain crs:Agent ;
  rdfs:label "associated with" ;
  rdfs:range crs:Agent ;
.
crs:controlNumber
  rdf:type owl:DatatypeProperty ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf org:identifier ;
.
crs:hasAgent
  rdf:type owl:ObjectProperty ;
  rdfs:comment "Whan an Association object indicates an association between two Agents, this property is used to indicate the Agent the assocition is to, e.g. Agent_1 -> associatedWith -> Association_X, Association_X -> hasAgent -> Agent_2."@en ;
  rdfs:domain crs:Association ;
  rdfs:label "has agent" ;
  rdfs:range crs:Agent ;
.
crs:hasRole
  rdf:type owl:ObjectProperty ;
  rdfs:domain crs:Association ;
  rdfs:range crs:Role ;
.
crs:mandatedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "mandated by" ;
  rdfs:range crs:Authority ;
.
crs:performs
  rdf:type owl:ObjectProperty ;
  rdfs:label "performs" ;
  rdfs:range <http://reference.data.gov.au/def/ont/agrif#Function> ;
.
crs:previousAgency
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf org:linkedTo ;
  owl:inverseOf crs:subsequentAgency ;
  owl:propertyChainAxiom (
      org:resultedFrom
      org:originalOrganization
    ) ;
.
crs:subordinateAgency
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf org:hasSubOrganization ;
  rdfs:subPropertyOf org:linkedTo ;
  owl:inverseOf crs:superiorAgency ;
.
crs:subsequentAgency
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf org:linkedTo ;
.
crs:superiorAgency
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf org:linkedTo ;
  rdfs:subPropertyOf org:subOrganizationOf ;
.
<http://orcid.org/0000-0002-8742-7730>
  rdf:type owl:NamedIndividual ;
.
terms:creator
  rdf:type owl:ObjectProperty ;
  rdfs:label "creator" ;
  skos:altLabel "was created by" ;
.
<http://reference.data.gov.au/def/ont/agrif#AGIFTFunction>
  rdf:type owl:Class ;
  rdfs:isDefinedBy <http://reference.data.gov.au/def/ont/agrif> ;
  rdfs:label "AGIFT Function" ;
  rdfs:subClassOf <http://reference.data.gov.au/def/ont/agrif#Function> ;
.
<http://reference.data.gov.au/def/ont/agrif#Function>
  rdf:type owl:Class ;
  rdfs:comment "A Function is a process that is performed routinely to carry out a part of the mandate of an Australian Government Agency." ;
  rdfs:isDefinedBy <http://reference.data.gov.au/def/ont/agrif> ;
  rdfs:label "Function of Government" ;
.
<http://reference.data.gov.au/def/ont/agrif#RecordsAuthorityFunction>
  rdf:type owl:Class ;
  rdfs:isDefinedBy <http://reference.data.gov.au/def/ont/agrif> ;
  rdfs:label "Records Authority Function" ;
  rdfs:subClassOf <http://reference.data.gov.au/def/ont/agrif#Function> ;
.
skos:altLabel
  rdf:type owl:AnnotationProperty ;
.
org:Membership
  rdf:type owl:Class ;
  rdfs:label "Membership" ;
  rdfs:subClassOf crs:Association ;
.
org:Organization
  rdf:type owl:Class ;
.
org:hasMember
  rdf:type owl:ObjectProperty ;
  rdfs:label "has member" ;
  rdfs:subPropertyOf crs:associatedWith ;
  owl:inverseOf org:member ;
.
org:identifier
  rdf:type owl:DatatypeProperty ;
.
org:linkedTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain org:Organization ;
  rdfs:label "linked to"@en ;
  rdfs:range org:Organization ;
  skos:altLabel "related organization" ;
.
org:member
  rdf:type owl:ObjectProperty ;
  rdfs:label "member" ;
  rdfs:subPropertyOf crs:associatedWith ;
.
<http://xmlns.com/foaf/0.1/Agent>
  rdf:type owl:Class ;
.
<http://xmlns.com/foaf/0.1/Person>
  rdf:type owl:Class ;
.
